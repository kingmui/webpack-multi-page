{
  "name": "Webpack-Multi-Page",
  "version": "1.0.0",
  "private": true,
  "description": "Webpack Multi-page development configuration",
  "homepage": "https://www.kingmui.cn/",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"./**/*.{css,less,sass,scss}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,less,sass,scss}\" --fix",
    "prettier": "prettier --write \"./**/*.{js,jsx,css,less,md,json}\"",
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kingmui/Webpack-Multi-Page.git"
  },
  "keywords": [
    "multi-page"
  ],
  "author": "kingmui<muikinghk@yahoo.com.hk>",
  "license": "ISC",
  "browserslist": [
    "Android >= 4.0",
    "ios >= 6",
    "Edge >= 10",
    "Firefox >= 20",
    "Safari >= 6",
    "Chrome >= 12",
    "ChromeAndroid >= 4.0",
    "QQAndroid >= 1.2",
    "not ie <= 9"
  ],
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --use-tabs --write",
      "git add"
    ],
    "**/*.{css,less}": [
      "stylelint --fix",
      "prettier --use-tabs --write",
      "git add"
    ],
    "**/*.{md,json}": [
      "prettier --use-tabs --write",
      "git add"
    ]
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-emoji": "^1.10.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "^1.18.2",
    "purify-css": "^1.2.5",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-html-plugin": "^0.1.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.9.0"
  }
}
